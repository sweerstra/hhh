{
  "name": "hhh",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "hhh",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "permissions": [
    "alarms",
    "notifications"
  ]
}