{
  "name": "hhh",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "src/background/index.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "hhh",
    "default_popup": "src/popup/popup.html"
  },
  "options_page": "src/options/options.html",
  "permissions": [
    "alarms"
  ]
}